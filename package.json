{
  "name": "josh-friede-website",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Personal website of Josh Friede, Software Engineer at Indeed",
  "author": "Josh Friede",
  "license": "MIT",
  "homepage": "https://joshfrie.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/joshua-friede/amplify-vite-website"
  },
  "keywords": [
    "react",
    "typescript",
    "vite",
    "aws-amplify",
    "personal-website",
    "software-engineer"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "test": "echo \"No tests specified\" && exit 0",
    "analyze": "npm run build && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.11.2",
    "aws-amplify": "^6.15.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.5.0",
    "@aws-amplify/backend-cli": "^1.2.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "aws-cdk": "^2.1021.0",
    "aws-cdk-lib": "^2.206.0",
    "constructs": "^10.4.2",
    "esbuild": "^0.25.8",
    "eslint": "^9.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.6.2",
    "tsx": "^4.7.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.5",
    "vite-bundle-analyzer": "^1.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
